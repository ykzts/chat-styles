    "node-sass": "^4.12.0",
    "prettier": "^1.18.2",
    "react-scripts": "^3.0.1",
    "typescript": "^3.5.3"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "homepage": "https://chat-styles.app/",
  "license": "MIT",
  "name": "@ykzts/chat-styles",

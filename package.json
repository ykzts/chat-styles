    "url": "https://github.com/ykzts/chat-styles.git"
  },
  "scripts": {
    "build": "react-scripts build",
    "dev": "react-scripts start",
    "format": "prettier --ignore-path .gitignore --write \"**/*.{html,js,json,md,scss,ts,tsx}\"",
